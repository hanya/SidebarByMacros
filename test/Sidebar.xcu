        <prop oor:name="HighContrastIconURL" oor:type="xs:string">
          <value>vnd.sun.star.extension://mytools.ui.test.SidebarByMacros/icon1.png</value>
        </prop>
        <prop oor:name="ContextList">
          <value oor:separator=";">
