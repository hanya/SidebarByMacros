        </prop>
        <prop oor:name="ContextList">
          <value oor:separator=";">
            Writer, any, visible ;
          </value>
        </prop>
        <prop oor:name="ImplementationURL" oor:type="xs:string">
          <value>private:resource/toolpanel/foo.bar/MyPanel1</value>
        </prop>
        <prop oor:name="OrderIndex" oor:type="xs:int">
          <value>100</value>
        </prop>
